t axios from 'axios'\n\nexport default {\n  setup() {\n    let data = reactive({\n      city: 'Longyan',\n      weather: null\n    })\n    \n    const apiUrl = 'http://localhost:3000'\n    const getWeather = () =>{\n      axios(`${ apiUrl }?units=metric&q=${ data.city }`).then(response=> {\n        data.weather = response.data\n        console.log(data.weather)\n      }) \n    }\n\n    return{\n      data,\n      getWeather\n    }\n  }\n}\n</script>\n<style lang=\"sass\">\n  .enter-city\n    input\n      font-size: 40px\n  .weather\n    margin-top: 10px \n    h1\n      font-size: 80px\n    h1,h2,h3\n      margin: 0\n</style>\n"],"sourceRoot":""}]}